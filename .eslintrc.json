{
  "parser": "@typescript-eslint/parser",
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
  },
  "plugins": [
    "react",
    "prettier",
    "@typescript-eslint"
  ],
  "parserOptions": {
    "version": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier/react",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
      "quotes": [2, "single"],
      "no-unused-vars": ["error", { "vars": "local" }],
      "no-undef": "error",
      "react/prop-types": 0,
      "no-prototype-builtins": 0,
      "react/display-name": 0,
      "react/no-string-refs": 0
  }
}
