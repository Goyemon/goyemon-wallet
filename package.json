  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write src",
      "git add"
    ],
    "*.js": [
      "eslint --fix src/**/*.js",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": "./jest.setup.js"
  },
  "react-native": {
